programa {
  funcao inicio() {
    
  //Declaração de variáveis.

  inteiro valorA, valorB, c
  

  //Solicitando dados ao usuário.

  escreva("Digite o valor de A: ")
  leia(valorA)

  escreva("Digite o valoe de B: ")
  leia(valorB)


  //Condições.

  se (valorA == valorB){
    c = valorA + valorB 
  } se (valorA > valorB){
    c = valorA * valorB
  } se (valorA < valorB)
    c = valorA * valorB

  //Exibindo Resultado
  escreva("C : ",c)
  }
}
